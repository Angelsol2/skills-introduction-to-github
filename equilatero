public class equilatero extends abstractT{
    private double lado;

    public equilatero(double lado) {
        this.lado = lado;
    }

    public double getLado() {
        return lado;
    }

    public void setLado(double lado) {
        this.lado = lado;
    }
    
    @Override
    double area(){
        return((lado*lado)*Math.sqrt(3))/4;
    }
}
